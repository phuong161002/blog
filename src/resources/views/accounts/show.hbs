<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Username</th>
            <th scope="col">Password</th>
            <th scope="col">Server</th>
            <th scope="col">Gender</th>
        </tr>
    </thead>
    <tbody>
        {{#each accounts}}
        <tr>
            <th scope="row">{{@index}}</th>
            <td>{{this.username}}</td>
            <td>{{this.password}}</td>
            <td>{{this.server}}</td>
            <td>{{this.gender}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>